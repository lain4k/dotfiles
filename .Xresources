Xft.dpi: 165

! special
*.foreground: #baad9b
*.background: #1E2326
*.cursorColor: #90a574

! black
*.color0: #1E2326
*.color8: #1E2326

! red
*.color1: #c56e74
*.color9: #c56e74

! green
*.color2: #90a574
*.color10: #90a574

! yellow
*.color3: #bca272
*.color11: #bca272

! blue
*.color4: #3883b2
*.color12: #3883b2

! magenta
*.color5: #b986a4
*.color13: #b986a4

! cyan
*.color6: #70a3a1
*.color14: #70a3a1

! white
*.color7: #e0dad5
*.color15: #e0dad5

!! URxvt Appearance
URxvt.font: xft:Monospace:style=Regular:size=11
URxvt.boldFont: xft:Monospace:style=Bold:size=11
URxvt.italicFont: xft:Monospace:style=Italic:size=11
URxvt.boldItalicFont: xft:Monospace:style=Bold Italic:size=11
URxvt.letterSpace: 0 
URxvt.lineSpace: 0 
URxvt.geometry: 400x400
URxvt.internalBorder: 24 
URxvt.cursorBlink: false
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false


!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
